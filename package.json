{
  "name": "flashcardsapp",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest ./__tests__",
    "test:watch": "jest --watch ./__tests__"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
